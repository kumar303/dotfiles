[user]
    name = Kumar McMillan
    email = kumar.mcmillan@gmail.com
[color]
    diff = auto
    status  = auto
    branch = auto
[alias]
    br = branch
    ci = commit
    co = checkout
    pp = !git remote prune origin && git pull
    st = status
    fix = "commit -a --amend -C HEAD"
    ri = rebase --interactive --autosquash HEAD~5
[core]
    excludesfile = /Users/kumar/.gitignore
[push]
    default = current
[help]
    autocorrect = 1
